{
    "name": "@anolilab/cerebro-notify-extension",
    "version": "1.0.0-alpha.1",
    "description": "Prettier shareable config for Anolilab.",
    "keywords": [
        "cerebro",
        "anolilab",
        "cli",
        "extension",
        "toolbox",
        "notify"
    ],
    "homepage": "http://anolilab.com/nodejs/packages/cerebro-package-manager-extension",
    "repository": "https://github.com/anolilab/cerebro.git",
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./package.json": "./package.json"
    },
    "main": "index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "dev:esm": "rimraf ./dist ./types && tsc --outDir dist --watch"
    },
    "dependencies": {
        "@types/node-notifier": "^8.0.1",
        "node-notifier": "^10.0.0"
    },
    "devDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.3",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.3"
    },
    "engines": {
        "node": ">=12"
    },
    "publishConfig": {
        "access": "public"
    }
}
