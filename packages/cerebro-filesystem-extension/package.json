{
    "name": "@anolilab/cerebro-filesystem-extension",
    "version": "0.0.0",
    "description": "Prettier shareable config for Anolilab.",
    "keywords": [
        "cerebro",
        "anolilab",
        "cli",
        "extension",
        "toolbox",
        "filesystem"
    ],
    "homepage": "http://anolilab.com/nodejs/packages/cerebro-filesystem-extension",
    "repository": "https://github.com/anolilab/cerebro.git",
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./package.json": "./package.json",
        "./": "./"
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "dev:esm": "rimraf ./dist ./types && tsc --outDir dist --watch"
    },
    "dependencies": {
        "fs-chmod": "1.0.6",
        "fs-extra": "10.0.0",
        "trash": "^7.2.0"
    },
    "devDependencies": {
        "@anolilab/cerebro-core": "^1.0.0-alpha.2",
        "fs-jetpack": "^4.1.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "@anolilab/cerebro-core": "^1.0.0-alpha.2",
        "fs-jetpack": "^4.1.0"
    },
    "engines": {
        "node": ">=12"
    },
    "publishConfig": {
        "access": "public"
    }
}
