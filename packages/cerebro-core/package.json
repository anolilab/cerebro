    "devDependencies": {
        "@types/node": "16.0.0",
        "@types/prompts": "^2.0.13",
        "@types/which": "^2.0.1",
        "mock-stdin": "1.0.0",
        "rimraf": "^3.0.2",
        "temp-write": "^5.0.0",
        "typescript": "^4.3.4",
