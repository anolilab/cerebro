        "hard-rejection": "^2.1.0",
        "has-yarn": "^2.1.0",
        "is-ci": "^3.0.0",
        "is-installed-globally": "^0.4.0",
        "is-yarn-global": "^0.4.0",
        "kind-of": "^6.0.3",
        "leven": "^3.1.0",
        "ora": "^5.4.1",
        "prompts": "^2.4.1",
        "strip-ansi": "7.0.0",
        "supports-color": "^9.0.1",
