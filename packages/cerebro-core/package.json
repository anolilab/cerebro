{
    "name": "@anolilab/cerebro-core",
    "version": "1.0.0-alpha.4",
    "description": "A delightful toolkit for building Node-powered CLIs.",
    "keywords": [
        "CLI",
        "command",
        "line",
        "class",
        "terminal",
        "ANSI",
        "cli",
        "opts",
        "nopt",
        "options",
        "args",
        "argv",
        "interactive"
    ],
    "homepage": "http://anolilab.com/nodejs/packages/cerebro",
    "repository": "https://github.com/anolilab/cerebro.git",
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de",
        "url": "https://github.com/prisis"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./commonjs/index.js"
        },
        "./esm": "./esm/index.js",
        "./commonjs": "./commonjs/index.js",
        "./package.json": "./package.json"
    },
    "main": "./commonjs/index.js",
    "module": "./esm/index.js",
    "types": "types/index.d.ts",
    "files": [
        "esm/",
        "commonjs/",
        "types/",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "dev:esm": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json --watch"
    },
    "dependencies": {
