{
    "name": "@anolilab/cerebro-strings-extension",
    "version": "1.0.0-alpha.2",
    "description": "Prettier shareable config for Anolilab.",
    "keywords": [
        "cerebro",
        "anolilab",
        "cli",
        "extension",
        "toolbox",
        "strings"
    ],
    "homepage": "http://anolilab.com/nodejs/packages/cerebro-strings-extension",
    "repository": "https://github.com/anolilab/cerebro.git",
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./commonjs/index.js"
        },
        "./esm": "./esm/index.js",
        "./commonjs": "./commonjs/index.js",
        "./package.json": "./package.json"
    },
    "main": "./commonjs/index.js",
    "module": "./esm/index.js",
    "types": "types/index.d.ts",
    "files": [
        "esm/",
        "commonjs/",
        "types/",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "dev:esm": "rimraf ./dist ./types && tsc --outDir dist --watch"
    },
    "dependencies": {
        "@types/lodash.camelcase": "^4.3.6",
        "@types/lodash.kebabcase": "^4.1.6",
        "@types/lodash.lowercase": "^4.3.6",
        "@types/lodash.lowerfirst": "^4.3.6",
        "@types/lodash.pad": "^4.5.6",
        "@types/lodash.snakecase": "^4.1.6",
        "@types/lodash.startcase": "^4.4.6",
        "@types/lodash.trimend": "^4.5.6",
        "@types/lodash.trimstart": "^4.5.6",
        "@types/lodash.uppercase": "^4.3.6",
        "@types/lodash.upperfirst": "^4.3.6",
        "@types/pluralize": "^0.0.29",
        "lodash.camelcase": "^4.3.0",
        "lodash.kebabcase": "^4.1.1",
        "lodash.lowercase": "^4.3.0",
        "lodash.lowerfirst": "^4.3.1",
        "lodash.pad": "^4.5.1",
        "lodash.snakecase": "^4.1.1",
        "lodash.startcase": "^4.4.0",
        "lodash.trimend": "^4.5.1",
        "lodash.trimstart": "^4.5.1",
        "lodash.uppercase": "^4.3.0",
        "lodash.upperfirst": "^4.3.1",
        "pluralize": "^8.0.0"
    },
    "devDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.4",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.4"
    },
    "engines": {
        "node": ">=12"
    },
    "publishConfig": {
        "access": "public"
    }
}
