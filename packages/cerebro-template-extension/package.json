{
    "name": "@anolilab/cerebro-template-extension",
    "version": "1.0.0-alpha.4",
    "description": "cerebro template extension - generating files from templates",
    "keywords": [
        "cerebro",
        "anolilab",
        "cli",
        "extension",
        "toolbox",
        "template"
    ],
    "homepage": "http://anolilab.com/nodejs/packages/cerebro-template-extension",
    "repository": "https://github.com/anolilab/cerebro.git",
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./commonjs/index.js"
        },
        "./esm": "./esm/index.js",
        "./commonjs": "./commonjs/index.js",
        "./package.json": "./package.json"
    },
    "main": "./commonjs/index.js",
    "module": "./esm/index.js",
    "types": "types/index.d.ts",
    "files": [
        "esm/",
        "commonjs/",
        "types/",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "build": "rimraf ./dist ./types && tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json && ./fixup.sh",
        "dev:esm": "rimraf ./dist ./types && tsc --outDir dist --watch"
    },
    "dependencies": {
        "@anolilab/cerebro-filesystem-extension": "1.0.0-alpha.5",
        "@anolilab/cerebro-strings-extension": "1.0.0-alpha.5",
        "@types/ejs": "^3.0.7",
        "ejs": "^3.1.6"
    },
    "devDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.7",
        "rimraf": "^3.0.2",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "@anolilab/cerebro-core": "1.0.0-alpha.7"
    },
    "engines": {
        "node": ">=12"
    },
    "publishConfig": {
        "access": "public"
    }
}
